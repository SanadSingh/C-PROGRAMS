# include <stdio.h>

int main()
{
    int i=1,n,a=0,b=1,c;
    
    printf("Enter the no. of terms in Fibonacci Series : \n");
    scanf("%d",&n);
    printf("\nThe Fibonacci Series is given below : \n");
    printf("0\n1\n");
    
    while(i<=(n-2))
    {  
        c=a+b;
        printf("%d\n",c);
        a=b;
        b=c;
        
        i+=1;
    }
    
    return(0);
}
